go env | grep GOCACHE

export GO111MODULE=on

go mod vendor

go fmt ./...&&go test ./... -v -mod=vendor

go fmt ./...&&go test ./tests/... -v -mod=vendor


GOOS=js GOARCH=wasm go build -o build/main.wasm


go fmt ./...&&go run main/basicws_server.go
go fmt ./...&&go run main/basicws_client.go

go fmt ./...&&go run main/logwatcher_server.go
go fmt ./...&&go run main/logwatcher_client.go
